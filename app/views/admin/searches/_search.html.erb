<% if admin_signed_in? %>
  <div class = search_form >
    <%= form_with url: admin_search_path, method: :get do |f| %>
      <%= f.text_field :word, style: "border-radius: 20px;" %>
      <%= f.select :range, options_for_select([["User"], ["Book"]])%>
      <%= f.submit "検索", class: "btn btn-primary" %>
    <% end %>
  </div>
<% end %> 